#! /bin/sh

bspc config border_width         3
onboard &
nm-applet &
blueman-applet &

# disable mobile network and bluetooth on boot
nmcli radio wwan off
# rfkill block bluetooth

# disable touchpad
xinput disable 'AlpsPS/2 ALPS DualPoint TouchPad'

# setup wacom tablet
$HOME/SystemSettings/wacom-custom.sh
