## General hotkeys
super + Return
	$HOME/.local/bin/term-open.sh main
super + shift + Return
	$HOME/.local/bin/term-open.sh home
super + t
	rofi -show ssh
super + shift + t
	st
super + d
	rofi -show combi -modi combi
Print
	flameshot gui
super + alt + Delete
	st -e htop
super + Pause
	$HOME/.local/bin/lockscript


# reload sxhkd
super + Escape
	pkill -USR1 -x sxhkd
