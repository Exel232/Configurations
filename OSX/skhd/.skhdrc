cmd - return : /Users/max/Configurations/OSX/open-terminal.scpt
cmd + shift - return : /Users/max/Configurations/OSX/open-terminal.scpt home

# chunkwm keybindings
cmd - w : chunkc tiling::window --close

cmd - h : chunkc tiling::window --focus west
cmd - j : chunkc tiling::window --focus south
cmd - k : chunkc tiling::window --focus north
cmd - l : chunkc tiling::window --focus east


cmd + shift - h : chunkc tiling::desktop --rotate 90
shift + cmd - j : chunkc tiling::window --warp next
shift + cmd - k : chunkc tiling::window --warp prev
cmd + shift - l : chunkc tiling::desktop --rotate 270

cmd - 1 : chunkwm_focus_desktop.sh 1
cmd - 2 : chunkwm_focus_desktop.sh 2
cmd - 3 : chunkwm_focus_desktop.sh 3
cmd - 4 : chunkwm_focus_desktop.sh 4
cmd - 5 : chunkwm_focus_desktop.sh 5
cmd - 6 : chunkwm_focus_desktop.sh 6
cmd - 7 : chunkwm_focus_desktop.sh 7

shift + cmd - 1 : chunkwm_move_window_desktop.sh 1
shift + cmd - 2 : chunkwm_move_window_desktop.sh 2
shift + cmd - 3 : chunkwm_move_window_desktop.sh 3
shift + cmd - 4 : chunkwm_move_window_desktop.sh 4
shift + cmd - 5 : chunkwm_move_window_desktop.sh 5
shift + cmd - 6 : chunkwm_move_window_desktop.sh 6
shift + cmd - 7 : chunkwm_move_window_desktop.sh 7

cmd - e : chunkc tiling::monitor -f prev
cmd - r : chunkc tiling::monitor -f next

shift + cmd - e  : chunkc tiling::window --send-to-monitor prev; chunkc tiling::monitor -f prev
shift + cmd - r : chunkc tiling::window --send-to-monitor next; chunkc tiling::monitor -f next

cmd - a : chunkc tiling::desktop --layout bsp
cmd + shift - a : chunkc tiling::window --toggle fullscreen
cmd - s : chunkc tiling::desktop --layout float
cmd + shift - s : chunkc tiling::window --toggle native-fullscreen

# cmd - space : chunkc tiling::desktop --layout monocle

ctrl + cmd - q : chunkc tiling::window --use-insertion-point cancel
ctrl + cmd - w : chunkc tiling::window --use-insertion-point north
ctrl + cmd - a : chunkc tiling::window --use-insertion-point west
ctrl + cmd - s : chunkc tiling::window --use-insertion-point south
ctrl + cmd - d : chunkc tiling::window --use-insertion-point east
